{
    "DocumentRoot": ".",
    "Port": "8080",
    "ThreadPoolSize" : "10",
    "AllowThreadModel" : "Pool|Multi|Single",
    "ThreadModel" : "Multi",
  "Plugins": [
    {
      "Path": "stat",
      "Class": "DNWS.StatPlugin",
      "Preprocessing": "true",
      "Postprocessing": "false",
      "Singleton": "false"
    },
    {
      "Path": "ox",
      "Class": "DNWS.OXPlugin",
      "Preprocessing": "false",
      "Postprocessing": "false"
    },
    {
      "Path": "delay",
      "Class": "DNWS.DelayPlugin",
      "Preprocessing": "false",
      "Postprocessing": "false"
    },
    {
      "Path": "twitter",
      "Class": "DNWS.TwitterPlugin",
      "Preprocessing": "false",
      "Postprocessing": "false"
    },
    {
      "Path": "twitterAPI",
      "Class": "DNWS.TwitterAPI",
      "Preprocessing": "false",
      "Postprocessing": "false"
    },
    {
      "Path": "user",
      "Class": "DNWS.TwitterAPI",
      "Preprocessing": "false",
      "Postprocessing": "false"
    },
    {
      "Path": "following",
      "Class": "DNWS.TwitterAPI",
      "Preprocessing": "false",
      "Postprocessing": "false"
    },
    {
      "Path": "tweet",
      "Class": "DNWS.TwitterAPI",
      "Preprocessing": "false",
      "Postprocessing": "false"
    }
  ]
}
